    "umd": "^3.0.3"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "clone": "^2.1.2",
    "convert-source-map": "^1.6.0",
    "depcheck": "^0.8.3",
    "source-map": "^0.7.3",
    "standard": "^12.0.1",
    "tape-promise": "^4.0.0",
    "uglify-js": "^3.4.9"
  },
  "scripts": {
