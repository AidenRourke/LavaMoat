  "main": "index.js",
  "version": "3.0.10",
  "dependencies": {
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "codemirror": "^5.52.2",
    "module-name-from-path": "^1.0.4",
    "ncp": "^2.0.0",
    "open": "^7.0.3",
    "pify": "^4.0.1",
    "react-codemirror2": "^7.1.0",
    "yargs": "^14.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "babel-loader": "8.0.5",
    "css-loader": "1.0.0",
    "d3": "^5.12.0",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "react": "^16.8.6",
    "react-bootstrap": "^1.0.0-beta.8",
    "react-dom": "^16.8.6",
    "react-force-graph": "^1.30.2",
    "react-hyperscript": "^3.2.0",
    "style-loader": "0.23.1",
    "webpack": "4.28.3",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "webpack-dev-server --config ./src/webpack.config.js",
