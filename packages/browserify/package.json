{
  "name": "lavamoat-browserify",
  "version": "8.2.0",
  "description": "browserify plugin for sandboxing dependencies with LavaMoat",
  "main": "src/index.js",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "engines": {
    "node": ">10.15.1"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "acorn-globals": "^4.3.3",
    "acorn-walk": "^6.1.1",
    "browserify-transform-tools": "^1.7.0",
    "clone": "^2.1.2",
    "combine-source-map": "^0.8.0",
    "cytoplasm": "^3.0.3",
    "fromentries": "^1.1.0",
    "json-stable-stringify": "^1.0.1",
    "merge-deep": "^3.0.2",
    "mkdirp": "^1.0.3",
    "module-name-from-path": "git+https://git@github.com/kumavis/module-name-from-path.git",
    "offset-sourcemap-lines": "^1.0.1",
    "pify": "^4.0.1",
    "resolve-package-path": "^1.2.7",
    "safe-buffer": "^5.1.2",
    "sesify-tofu": "^2.0.4",
    "through2": "^3.0.0",
    "umd": "^3.0.3"
  },
  "devDependencies": {
    "bify-package-factor": "^1.0.7",
    "browserify": "^16.2.3",
    "convert-source-map": "^1.6.0",
    "depcheck": "^0.8.3",
    "pump": "^3.0.0",
    "rimraf": "^3.0.2",
    "ses": "git+https://git@github.com/agoric/ses#fdb393f817f6124c4d055bff26b68e4cbe17618e",
    "source-map": "^0.7.3",
    "standard": "14.3.1",
    "tape": "^4.13.2",
    "tape-promise": "^4.0.0",
    "tmp": "^0.1.0",
    "uglify-js": "^3.4.9",
    "watchify": "^3.11.1"
  },
  "scripts": {
    "test": "node test",
    "lint": "npm run lint:standard && npm run lint:deps",
    "lint:standard": "standard src/**/*.js test/**/*.js",
    "lint:fix": "standard src/**/*.js test/**/*.js --fix",
    "lint:deps": "depcheck --ignore-dirs=lib --ignores='bify-package-factor,ses,banana,two,one,victim,attacker'",
    "build:ses": "(cd ./node_modules/ses && npm i && npm run build && cp ./dist/ses.umd.js ../../lib/)"
  },
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MetaMask/lavamoat-browserify.git"
  },
  "bugs": {
    "url": "https://github.com/MetaMask/lavamoat-browserify/issues"
  },
  "homepage": "https://github.com/MetaMask/lavamoat-browserify#readme",
  "standard": {
    "ignore": [
      "/src/sourcemaps.js",
      "/test/fixtures",
      "/src/preludeTemplate.js"
    ]
  }
}
